{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "types": [
      "nativewind/types",
      "jest"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "src",
    "App.tsx",
    "nativewind-env.d.ts",
    "jest",
    "src/**/*.test.tsx",
    "src/**/*.test.ts"
  ]
}